{
  "name": "vsc-presence-server",
  "version": "1.0.1",
  "description": "VS Code Presence Server",
  "repository": "https://github.com/BRAVO68WEB/vsc-presence-server.git",
  "main": "src/server.ts",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "dev": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" --files \"src/server.ts\" --signal SIGKILL",
    "start": "npm run build && node build/server.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "keywords": [],
  "author": {
    "email": "me@bravo68web.me",
    "name": "Jyotirmoy Bandyopadhayaya",
    "url": "https://itsmebravo.dev"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.3",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "redis": "^4.6.6",
    "supertest": "^6.3.3",
    "ws": "^8.13.0"
  }
}
